<h1>Sign In</h1>


<%= form_for(:session, url: sessions_path) do |f| %>
    <%=  f.label :email %>
    <%=  f.text_field :email %>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password%>
    <br>
    <%= f.submit "Sign in" %>
<% end %>

<p>Not already registered? <%= link_to "Sign Up Here", signup_path %></p>
