<!DOCTYPE html>
<html>
<head>
  <title>ToolShare</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

    <%= render 'layouts/header' %>

  <! add flash message >
  <div class="container">
    <% flash.each do |key,value|%>
      <div class="alert alert-<%= key %>"><%= value %> </div>
    <% end %>
  </div>

<%= yield %>

</body>
</html>
